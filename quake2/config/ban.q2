unbindall

alias Update "Echo Last Update 26.01.2000"

//=Server info=                      =Player info=
set hostname "ê‘he »all ÔÊ  ucticeë"
set deathmatch 1;                    set name "TJ.BANSHEE"
set dmflags 1072;                    set skin Female/Cobalt
set sv_reconnect_limit 3;            set gender_auto 0
set sv_maxclients 10;                set gender male
set cheats 0
