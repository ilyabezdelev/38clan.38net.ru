//=GL Video Settings=
set vid_ref gl;        set gl_texturemode GL_NEAREST_MIPMAP_NEAREST
set gl_mode 2;         set gl_ext_pointparameters 1
set gl_3dlabs_broken 1;set gl_ext_compiled_vertex_array 1
