//=Control=
bind mouse1 +Attack;   bind 1  use Blaster
bind mouse2 +Double;   bind e  use Rocket Launcher
bind w      +Forward;  bind r  use RailGun
bind s      +Back;     bind x  "use Grenades;use Grenade Launcher"
bind a      +MoveLeft; bind F1 "Ready;sv allready";      bind n skins
bind d      +MoveRight;bind F2 "notready;sv allnotready";bind [ Invprev         
