bind ctrl   +zoom;     bind F6 snd_restart;              bind = Sizeup
bind m      Menu;      bind F7 s_volume 0.4;             bind - Sizedown
bind Enter  Invuse;    bind F8 s_volume 1;               bind Pause "stop;quit"

//=OSP-TOURNEY-MODE=Server Commands & Team Play & Gladiator Bots=
set allow_item_powerscreen 1
set allow_item_powershield 1
set allow_item_quad 1
set allow_item_invul 1
set allow_item_pack 1
set allow_weapon_bfg10k 1
set allow_id 0
set client_hud 1
set match_countdown 10
set team_lockskin 0
set team_idteam 1
set match_startsound 0

set team_a_name "KomaHDa-1"
