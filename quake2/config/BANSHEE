 //BANSHEE<C>'S configuration file for Quake II
 //Read only

 unbindall

 //Player info

 set skin Female/Stiletto
 set name BANSHEE<C>
 set hostname "<îo óeòvår>"

 //Control

 bind mouse1  "+Attack"
 bind mouse2  "+double"
 bind w       "+Forward"
 bind s       "+Back"
 bind a       "+MoveLeft"
 bind d       "+MoveRight"
 bind Shift   "+MoveDown"
 bind space   "+die"
 bind tab     "+score"
 bind ctrl    "+zoom"
 bind e "use Rocket Launcher"
 bind r "use RailGun"
 bind f "use ChainGun"
 bind z "use Grenades;use Grenade Launcher"
 bind 1 "use Blaster"
 bind [ "Invprev"
 bind ] "Invnext"
 bind ' "Invdrop"
 bind Pause "stop;exec defult.cfg;quit"
 bind = "sizeup;play items/n_health.wav"
 bind - "sizedown;play items/n_health.wav"
 bind Enter "Invuse"
 bind F1 "Menu_StartServer"
 bind F2 "Menu_JoinServer"
 bind F3 "ScreenShot"
 bind F4 "exec banshee"
 bind F5 "f5"
 bind F6 "f6"
 bind F7 "Ha4Hem?"
 bind F8 ""
 bind F9 "tourney"
 bind F10 "sv addrandom"
 bind m "menu"
 bind q "h"
 bind c "b"

 //Aliases

 alias dm1 "map q2dm1"
 alias dm2 "map q2dm2"
 alias dm3 "map q2dm3"
 alias dm4 "map q2dm4"
 alias dm5 "map q2dm5"
 alias dm6 "map q2dm6"
 alias dm7 "map q2dm7"
 alias dm8 "map q2dm8"

 alias +score "score"
 alias -score "score"

 alias +zoom "sensitivity 2.4;fov 50"
 alias -zoom "sensitivity 6;fov 90"

 alias +double "+moveup;wait;wait;wait;-moveup;wait;wait;wait;+moveup"
 alias -double "-moveup"

 alias +die "use ShotGun;use MachineGun;use HyperBlaster;use Super ShotGun;use ChainGun;+Attack"
 alias -die "-attack"

 alias f5 "f5a"
 alias f5a "ready;alias f5 f5b" 
 alias f5b "sv allready;alias f5 f5a"

 alias f6 "f6a"
 alias f6a "He roToB;f6b"
 alias f6b "sv allnotready"

 alias ctf "bind Alt Use Grapple;id"
 alias tourney "game tourney;skill 3;e"
 alias e "e1"
 alias e1 "alias e e2;exec 1v1"
 alias e2 "alias e e3;exec dm"
 alias e3 "alias e e4;exec qual"
 alias e4 "alias e e5;exec rail"
 alias e5 "alias e e1;exec team"

 alias h "h1"
 alias h1 "hehe;alias h h2"
 alias h2 "Rulezzz!;alias h h3"
 alias h3 "|);alias h h4"
 alias h4 "=);alias h h1"

 alias b "b1"
 alias b1 "E6Tb;alias b b2"
 alias b2 "XpeHoBo;alias b b3"
 alias b3 ":{;alias b b4"
 alias b4 "|(;alias b b1"

