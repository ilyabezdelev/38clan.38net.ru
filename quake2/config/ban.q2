set m_yaw 0.022;  set g_select_empty 0;set vid_fullscreen 1; set s_primary 1
set m_pitch 0.017;set in_joystick 0;   set viewsize 80;      set s_mixahead 0.1
set in_mouse 1;   set cl_vwep 1;       set vid_gamma 0.5;    set s_loadas8bit 0
